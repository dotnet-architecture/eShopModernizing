<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  https://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <connectionStrings>
    <add name="CatalogDBContext" connectionString="Server=tcp:127.0.0.1,5433;Initial Catalog=Microsoft.eShopOnContainers.Services.CatalogDb;User Id=sa;Password=Pass@word" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="KeyVaultName" value="" />
    <add key="UseMockData" value="true" />
    <add key="UseCustomizationData" value="false" />
    <add key="StorageConnectionString" value="#YourAzureStorageCatalogConnectionString#" />
    <add key="UseAzureManagedIdentity" value="false" />
    <add key="UseAzureStorage" value="false" />
    <add key="UseAzureActiveDirectory" value="false" />
    <add key="AzureActiveDirectoryClientId" value="[Enter client ID as obtained from Azure Portal, e.g. 82692da5-a86f-44c9-9d53-2f88d52b478b]" />
    <add key="AzureActiveDirectoryTenant" value="[Enter tenant name, e.g. contoso.onmicrosoft.com]" />
    <add key="AzureActiveDirectoryInstance" value="https://login.microsoftonline.com/{0}" />
    <add key="PostLogoutRedirectUri" value="https://localhost:44322/" />
  </appSettings>
</configuration>